{
	"name": "open-reusable",
	"displayName": "Open a reusable",
	"description": "Open a variable or reusable referenced in the selected text",
	"icon": "images/open-reusable-icon.png",
	"version": "1.3.0",
	"publisher": "hubwriter",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:open-reusable",
		"onCommand:copy-reusable"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "open-reusable",
				"title": "Open reusable file"
			},
			{
				"command": "copy-reusable",
				"title": "Copy the reusable at the cursor position"
			}			
		],
        "keybindings": [
            {
                "command": "open-reusable",
                "key": "ctrl+alt+o",
                "mac": "ctrl+cmd+o"
			},
            {
                "command": "copy-reusable",
                "key": "ctrl+alt+c",
                "mac": "ctrl+cmd+c"
            }
        ]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^8.0.3",
		"@types/node": "^14.11.5",
		"eslint": "^7.28.0",
		"glob": "^7.1.6",
		"mocha": "^9.0.3",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"repository": {
        "type": "git",
        "url": "https://github.com/hubwriter/open-reusables/"
	},
	"license": "see the LICENSE file",
    "dependencies": {
    }
}
